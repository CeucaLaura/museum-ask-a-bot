{
  "compilerOptions": {
    "allowJs": true,
    "allowSyntheticDefaultImports": true,
    "allowUnreachableCode": false,
    "alwaysStrict": true,
    "checkJs": false,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "incremental": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "lib": [
      "dom",
      "es2017"
    ],
    "module": "esnext",
    "moduleResolution": "node",
    "noEmit": true,
    "noFallthroughCasesInSwitch": true,
    "noImplicitAny": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "preserveConstEnums": true,
    "removeComments": false,
    "resolveJsonModule": true,
    "skipLibCheck": true,
    "sourceMap": true,
    "strict": true,
    "target": "esnext",
    "baseUrl": "../",
    "outDir": ".next",
    "paths": {
      "~/*": ["./app/*"]
    },
    "tsBuildInfoFile": "./buildcache/app",
    "typeRoots": ["../node_modules/@types"]
  },
  "exclude": ["../server/**/*.*", "./**/*.test.*", "./.next"],
  "include": [
    "../index.d.ts",
    "./**/*"
  ]
}
